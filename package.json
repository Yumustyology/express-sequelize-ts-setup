{
  "name": "squlize-express",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --exec \"tsx src/index.ts\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write .",
    "migrate": "sequelize db:migrate --config src/config/sequelize-config.cjs --migrations-path src/migrations",
    "migrate:undo": "sequelize db:migrate:undo --config src/config/sequelize-config.cjs --migrations-path src/migrations",
    "migrate:undo:all": "sequelize db:migrate:undo:all --config src/config/sequelize-config.cjs --migrations-path src/migrations",
    "migration:create": "sequelize migration:generate --name",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo:all": "ts-node --esm ./node_modules/sequelize-cli/lib/sequelize db:seed:undo:all",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "migration:new": "node src/scripts/create-migration.cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.3",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.23",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
